        <script src="{{ asset('assets/bootstrap/js/bootstrap.js') }}"></script>
        <script src="{{ asset('assets/bootstrap/js/all.js') }}"></script>
        <script src="{{ asset('assets/fontawesome/js/all.js') }}"></script>

        {% endblock %}
