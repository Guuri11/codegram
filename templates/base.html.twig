<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Codegram{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/bootstrap/css/bootstrap.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/bootstrap/css/bootstrap-grid.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/bootstrap/css/bootstrap-reboot.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/fontawesome/css/all.min.css') }}">

        <link href="{{ asset('assets/fontawesome/css/all.min.css')}}" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        
        <script src="{{ asset('assets/bootstrap/js/jquery-3.5.1.min.js') }}"></script>
